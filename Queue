//simple operations
#include <iostream>
#include <queue>
using namespace std;

int main()
{
  queue<int> myQueue;
  
  for (int i=0; i< 10; i++)
  {
      cout << "enqueue " << i << endl;
      myQueue.push(i);
  }
  
  cout << "size of queue " << myQueue.size() << endl;
  
  cout << "the back of queue: " << myQueue.back() << endl;
  
  while (!myQueue.empty())
  {
      cout << "dequeue " << myQueue.front() << endl;
      myQueue.pop();
  }
  
  cout << "size of queue " << myQueue.size() << endl;
}
