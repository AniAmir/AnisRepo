//push, pop, empty, top, print
#include <iostream>
#include <stack>

using namespace std;

int main()
{
	stack<int> myStack;
	myStack.push(5);
	myStack.push(3);
	myStack.push(2);
	myStack.push(1);
	myStack.push(0);

	cout << "Number of ints in the stack " << myStack.size() << endl;
	while (!myStack.empty())
	{
		cout << "popping " << myStack.top() << endl;
		myStack.pop(); //pop the top member
	}
	cout << "Number of ints in the stack " << myStack.size() << endl;
    return 0;
}
